{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./build/",
        "sourceMap": true,
        "target": "es5",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "baseUrl": "src"
    },
    "include": ["./src/**/*"],
    "exclude": ["build", "node_modules"]
}
